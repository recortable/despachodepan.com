---
title: procesos
---

<% view = process_view(0) %>
<div id="wrapper" class="autoscroll" data-scrollTime="<%= view.scroll_time %>">
  <div id="grid">
    <div id="content">
      <% view.cards.each do |card| %>
          <% if displayable_card?(card) %>
              <%= pinta_card(card) %>
              <%= pinta_milestones(card) %>
          <% end %>
      <% end %>
      <div id="preview" style="display: none">&nbsp;</div>
      <div id="loading" style="display: none">
        <div id="spinner"></div>
      </div>
    </div>
  </div>
</div>

<div id="tags-bar">
  <ul id="tags" style="">
    <% view.tags.each do |tag| %>
        <li class="tag" id="tag_<%= tag.id %>" data-card-ids="<%= tag_ids(tag) %>">
            <a id="tag-<%= tag.id %>" class="tag tag-link" href="#" style="color: <%= "##{tag.color.value}" %>;"><%= tag.name %></a>
            <label id="ind-<%= tag.id %>" class="tag-indicator tag-link"
                   style="color: <%= "##{tag.color.value}" %>; border-color: <%= "##{tag.color.value}" %>">
              <%= tag.name %>
            </label>
        </li>
    <% end %>
  </ul>
</div>
